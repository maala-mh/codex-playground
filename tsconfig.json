{
  "compilerOptions": {
    "jsx": "react-native",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "node",
    "noEmit": true,
    "isolatedModules": true,
    "allowJs": true,
    "strict": true,
    "target": "ESNext",
    "types": ["react", "react-native"],
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "dist"]
}
